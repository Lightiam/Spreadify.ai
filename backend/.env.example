ICE_SERVERS=[{"urls":["stun:stun.l.google.com:19302"]}]

# Stripe Configuration
STRIPE_SECRET_KEY=your-stripe-secret-key

# Database Configuration
DATABASE_URL=sqlite:///./app.db
